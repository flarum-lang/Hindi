	"homepage": "https://github.com/flarum-lang/hindi",
	"keywords": [
		"locale",
		"flarum",
		"hindi-locale"
	],
	"license": "MIT",
	"authors": [
		{
			"name": "Your Name",
			"email": "your@email.net",
			"homepage": "https://github.com/flarum-lang/hindi"
		}
	],
	"support": {
